{
	"name": "pl_mysql_to_csv",
	"properties": {
		"activities": [
			{
				"name": "For Each Table",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.p_table_names",
						"type": "Expression"
					},
					"isSequential": false,
					"activities": [
						{
							"name": "MySQL to CSV",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "MySqlSource"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".csv"
									}
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": true,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "ds_mysql_source",
									"type": "DatasetReference",
									"parameters": {
										"p_table_name": {
											"value": "@item()",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "ds_csv_file",
									"type": "DatasetReference",
									"parameters": {
										"p_folder_name": "europe/sink",
										"p_file_name": {
											"value": "@concat('sql_',toLower(item()),'csv')",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"p_table_names": {
				"type": "array",
				"defaultValue": [
					"Country",
					"Player",
					"Team",
					"League",
					"Match_Record"
				]
			}
		},
		"folder": {
			"name": "Bronze Ingestion"
		},
		"annotations": []
	}
}