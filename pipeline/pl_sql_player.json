{
	"name": "pl_sql_player",
	"properties": {
		"activities": [
			{
				"name": "Player from MySQL to CSV",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "MySqlSource"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "id",
									"type": "Int32"
								},
								"sink": {
									"name": "id",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "player_api_id",
									"type": "Int32"
								},
								"sink": {
									"name": "player_api_id",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "player_name",
									"type": "String"
								},
								"sink": {
									"name": "player_name",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "player_fifa_api_id",
									"type": "Int32"
								},
								"sink": {
									"name": "player_fifa_api_id",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "player_birthday",
									"type": "DateTime"
								},
								"sink": {
									"name": "player_birthday",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "player_height",
									"type": "Int32"
								},
								"sink": {
									"name": "player_height",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "player_weight",
									"type": "Int32"
								},
								"sink": {
									"name": "player_weight",
									"type": "String",
									"physicalType": "String"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_sql_source",
						"type": "DatasetReference",
						"parameters": {
							"Table": "Player"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_sink",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Bronze Ingestion"
		},
		"annotations": []
	}
}